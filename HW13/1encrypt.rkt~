; Encryption

(define encrypt
	(lambda (str)

(define encrypt-vowels
  (lambda (c)


; Decryption

(define decrypt
	(lambda (str)

(define decrypt-vowels
  (lambda (c)

; Tests
(encrypt "Zebra is a horse")
(decrypt (encrypt "Zebra is a horse"))
(encrypt "COMPUTER science")
(decrypt (encrypt "COMPUTER science"))

